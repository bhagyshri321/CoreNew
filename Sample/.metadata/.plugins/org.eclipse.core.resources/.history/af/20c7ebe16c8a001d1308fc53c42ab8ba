package in.imp.String;

public class Practice {

	public static void main(String[] args) {
		String s= "Bhagyshri Saxena";
		String k= s.toLowerCase();
		int count=0;
		char[]t= k.toCharArray();
		
		for(char S:t) {
			count=0;
			for(int i=0;i<=k.length();i++) {
				if(S==k.charAt(i)) {
					count++;
				}
			}
			System.out.println(S+" "+count);
		}
	}

}
