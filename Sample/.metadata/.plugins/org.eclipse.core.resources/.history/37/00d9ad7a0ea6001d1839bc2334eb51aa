package in.imp.String;

public class StringTest {
	public static void main(String[] args) {
		
		String s= "this is java";
		String[] k= s.split(" ");
		System.out.println(k.length);
		
		
		char[] j= s.toCharArray();
		int count=0;
		for(char t:j) {
			for(int i=0;i<j.length;i++) {
				if(j[i]!=' ') {
				if(t==j[i]) {
					count++;
				}
			}}
			System.out.println(t+ " "+count);
			count=0;
		}
	}

}
