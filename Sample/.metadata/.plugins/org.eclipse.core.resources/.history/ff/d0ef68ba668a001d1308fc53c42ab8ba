package in.imp.String;

public class Practice {

	public static void main(String[] args) {
		
		String s= "Bhagyshri Saxena";
		int count=0;
		char[] b= s.toCharArray();
		for (char t:b) {
			/* count=0; */
			for (int i = 0; i < b.length; i++) {
				if (t==b[i]) {
					count++;
					
				}
				
			}
			System.out.println(t+"="+ count);
			
		}
	}

}
