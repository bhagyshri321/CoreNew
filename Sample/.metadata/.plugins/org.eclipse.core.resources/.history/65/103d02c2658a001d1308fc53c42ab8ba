package in.imp.String;

public class ImpString1 {
	
	public static void main(String[] args) {
		
		String s= "Bhagyshri";
		int count=0;
		char[]c= s.toCharArray();
		for(char t:c) {
			for(int i=0;i<c.length;i++) {
			if (t==c[i]) {
				count++;
				
			}
				
			}
			System.out.println(t+" "+count);
			count=0;
			
		}
		
	}

}
