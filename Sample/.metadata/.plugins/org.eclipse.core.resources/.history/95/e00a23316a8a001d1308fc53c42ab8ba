package in.imp.String;

public class Practice {

	public static void main(String[] args) {
		
		String b= "Bhagyshri Saxena";
		
		String[] c= b.split(" ");
		for(String K:c) {
			System.out.println(K);
			for(int i=K.length()-1;i>=0;i--) {
				
				System.out.print(K.charAt(i));
			}
			System.out.println();
		}
		
	}

}
