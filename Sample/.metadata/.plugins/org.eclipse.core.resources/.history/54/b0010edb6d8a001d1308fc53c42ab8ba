package in.imp.String;

public class Practice {

	public static void main(String[] args) {
		String s= "Bhagyshri Saxena";
		
		int count=0;
		char[]t= s.toCharArray();
		
		for(char S:t) {
			count=0;
			for(int i=0;i<s.length();i++) {
				if(S==s.charAt(i)) {
					count++;
				}
			}
			System.out.println(S+" "+count);
		}
	}

}
