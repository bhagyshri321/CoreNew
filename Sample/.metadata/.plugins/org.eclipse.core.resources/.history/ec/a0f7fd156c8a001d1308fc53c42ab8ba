package in.imp.String;

public class Practice {

	public static void main(String[] args) {
		String s= "Bhagyshri Saxena";
		String k= s.toLowerCase();
		 int count =0;
		 
		 for(char a='a';a<='z';a++) {
			 for(int i=0;i<k.length();i++) {
				 if (a==k.charAt(i)) {
					 count++;
					
				}
			 }
			 if (count!=0) {
				 System.out.println(a+"="+count);
				
			}
		 }
	}

}
