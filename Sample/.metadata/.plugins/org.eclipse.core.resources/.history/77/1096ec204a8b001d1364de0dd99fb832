package in.imp.String;

public class Practice {

	public static void main(String[] args) {

		int[]a= {6,5,7,8,2,3,1};
		
		int temp;
		for(int i=0;i<a.length;i++) {
			for(int j=i+1;j<a.length;j++) {
				if(a[j]>a[i]) {
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
					System.out.println(a[i]);
					System.out.println(a[j]);
					
				}
			}
		}
	}}

	


