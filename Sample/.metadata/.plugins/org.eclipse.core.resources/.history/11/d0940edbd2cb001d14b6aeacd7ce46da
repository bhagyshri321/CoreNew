package in.co.testException;

public class Test {
	
	public static void main(String[] args) {
		
		String s= "Bhag";
		
		try{System.out.println(s.charAt(6));}
		
		catch(StringIndexOutOfBoundsException e) {
			System.out.println("String is small");
			System.exit(0);
		}
		
		finally {
			System.out.println("I am final");
		}
	}

}
