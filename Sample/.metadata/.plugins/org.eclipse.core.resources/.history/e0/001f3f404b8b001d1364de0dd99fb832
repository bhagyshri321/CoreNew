package in.imp.String;

public class Practice {

	public static void main(String[] args) {

		int[]a= {2,4,5,7,9,7,2};
		int temp;
		for(int i=0;i<a.length;i++) {
			for(int k=i+1;k<a.length;k++) {
				
				if(a[k]>a[i]) {
				temp=a[i];
				a[i]=a[k];
				a[k]=temp;
				
				}
			}
		}System.out.println(a[1]);
	}}

	


